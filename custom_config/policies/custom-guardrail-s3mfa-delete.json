{
    "Version":"2012-10-17",
    "Statement":[
       {
          "Effect":"Deny",
          "Action":[
             "s3:DeleteObject",
             "s3:DeleteBucket"
          ],
          "Resource":[
             "*"
          ],
          "Condition":{
             "BoolIfExists":{
                "aws:MultiFactorAuthPresent":[
                   "false"
                ]
             },
             "StringNotLike":{
                "aws:PrincipalARN":"arn:aws:iam::111111111111:role/DATASYNC_ROLE_NAME"
             }
          }
       }
    ]
 }